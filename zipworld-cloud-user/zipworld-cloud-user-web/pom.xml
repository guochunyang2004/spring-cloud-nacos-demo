<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <artifactId>zipworld-cloud-user</artifactId>
        <groupId>cn.zipworld.cloud</groupId>
        <version>1.0.0</version>
        <relativePath>../pom.xml</relativePath>
    </parent>
    <groupId>cn.zipworld.cloud</groupId>
    <artifactId>zipworld-cloud-user-web</artifactId>
    <version>1.0.0</version>
    <name>zipworld-cloud-user-web</name>
    <description>Demo project for Spring Boot</description>
    <packaging>jar</packaging>
    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
            <version>${spring-boot.version}</version>
        </dependency>
        <dependency>
            <groupId>cn.zipworld.cloud</groupId>
            <artifactId>zipworld-cloud-user-service</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>cn.zipworld.cloud</groupId>
            <artifactId>zipworld-cloud-user-service-impl</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
            <exclusions>
                <exclusion>
                    <groupId>org.junit.vintage</groupId>
                    <artifactId>junit-vintage-engine</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <!--加载依赖模块-->
        <dependency>
            <groupId>cn.zipworld.cloud</groupId>
            <artifactId>zipworld-cloud-common-config</artifactId>
            <version>1.0.0</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>cn.zipworld.cloud</groupId>
            <artifactId>zipworld-cloud-common-vo</artifactId>
            <version>1.0.0</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>cn.zipworld.cloud</groupId>
            <artifactId>zipworld-cloud-common-util</artifactId>
            <version>1.0.0</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>cn.zipworld.cloud</groupId>
            <artifactId>zipworld-cloud-user-vo</artifactId>
            <version>1.0.0</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>cn.zipworld.cloud</groupId>
            <artifactId>zipworld-cloud-common-auth</artifactId>
            <version>1.0.0</version>
            <scope>compile</scope>
        </dependency>
        <!--加载依赖模块结束-->
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>${maven-surefire-plugin.version}</version>
                <configuration>
                    <skipTests>true</skipTests>    <!--默认关掉单元测试 -->
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>
